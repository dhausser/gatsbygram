{
  "name": "gatsbygram",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Davy Hausser <davy.hausser@gmail.com>",
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@emotion/core": "^10.0.28",
    "@types/express": "^4.17.4",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.32",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gatsby": "^2.20.12",
    "gatsby-design-tokens": "^2.0.4",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-emotion": "^4.2.1",
    "gatsby-plugin-layout": "^1.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-json": "^2.3.1",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "hex2rgba": "^0.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "qs": "^6.9.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0",
    "theme-ui": "^0.3.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "prettier": "^2.0.2"
  },
  "homepage": "https://davyhausser.com",
  "keywords": [
    "gatsby",
    "davyhausser"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "engines": {
    "node": ">=13.12.0"
  },
  "eslintIgnore": [
    "interfaces",
    "**/__tests__/fixtures/"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --ext .js,.jsx,.ts,.tsx --fix",
      "git add"
    ],
    "*.{md,css,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.svg": [
      "svgo --pretty --indent=2 --config=svgo.yml --multipass",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged || node scripts/on-lint-error.js"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-0"
  }
}
